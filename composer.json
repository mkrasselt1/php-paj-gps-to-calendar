{
    "name": "mkrasselt/paj-gps-to-calendar",
    "description": "PAJ Finder GPS to Calendar Integration Tool",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PajGpsCalendar\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.0",
        "sabre/dav": "^4.4",
        "symfony/console": "^6.0",
        "symfony/yaml": "^6.0",
        "doctrine/dbal": "^3.0",
        "monolog/monolog": "^3.0"
    },
